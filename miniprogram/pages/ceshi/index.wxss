/**index.wxss**/
.container{
  width: 700rpx;
  border-radius: 20rpx;
}
.n-popup-con{
  padding: 30rpx 0 0rpx 38rpx;
  overflow: auto;
}
.van-center-enter-to{
  border-radius: 50rpx;
}
.n-popup-foot{
  position: fixed;
  width: 100%;
  height: 90rpx;
  left:0;
  bottom:0;
  display: flex;
  background-color: #faf5f5;
  border-top:1px solid #eeeded;
  padding: 10rpx 30rpx 0 0;
}
.n-popup-f-btn{
  width: 160rpx;
}
.n-popup-f-delete{
  flex-grow: 1;
}
.n-con-button-red{
  background-color: #2caa65 !important;
  color:#ffffff;
}
.n-con-button-red:hover{
  background-color: #1b9653 !important;
  color:#ffffff;
}
.n-con-button-gray{
  background-color: #e6e6e6 !important;
}
.n-popup-f-delete{
  padding: 10rpx 0 0 50rpx;
}
.n-popup-f-delete .iconfont{
  font-size: 46rpx;
  color:#b5a095;
  display: block;
}
.n-popup-f-delete .iconfont.icon-shanchukai{
  color:#e2590e;
  margin-top:-5rpx ;
  margin-left: 3rpx;
}